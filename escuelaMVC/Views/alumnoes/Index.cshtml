@model IEnumerable<escuelaMVC.Models.alumno>

@{
    ViewBag.Title = "Index";
}


<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-9" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Alumnos de la Institucion</h1>
            <p class="lead">Aca encuentras toda la lista de los alumnos matriculados y sus carreras.</p>
        </div>

        <div class="col-md-3">
            <img  style="height: 300px; width: 250px; margin-right: 50px; " src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>


<p style="font-size: 16px;">
    <a class="btn btn-primary" @Html.ActionLink("Agregar Alumno", "Create")
    </a>
</p>

<table class="table" style="font-size: 18px;">
    <tr  style=" color: #ef6c00;">
        <th>
            Nombre
        </th>
        <th>
            Edad
        </th>
        <th>
            Semestre en Curso
        </th>
        <th>
            Genero
        </th>
        <th>
            Carrera en Curso
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nom_alu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.edad_alu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.sem_alu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.gen_alu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.carrera.nom_c)
            </td>
            <td style="color: #ef6c00;">
                @Html.ActionLink("Editar", "Edit", new { id = item.mat_alu }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.mat_alu })
            </td>
        </tr>
    }

</table>
