@model escuelaMVC.Models.alumno

@{
    ViewBag.Title = "Delete";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-8" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">¿Estas seguro?</h1>
            <p class="lead">Eliminaras un alumno, esperamos regrese pronto.</p>
        </div>

        <div class="col-md-4">
            <img style="height: 300px; width: 250px; margin-right: 50px; " src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>

<div>
    <hr />
    <dl class="dl-horizontal" style="font-size:18px;">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nom_alu)
        </dd>

        <dt>
            Edad
        </dt>

        <dd>
            @Html.DisplayFor(model => model.edad_alu)
        </dd>

        <dt>
            Semestre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sem_alu)
        </dd>

        <dt>
            Genero
        </dt>

        <dd>
            @Html.DisplayFor(model => model.gen_alu)
        </dd>

        <dt>
            Carrera
        </dt>

        <dd>
            @Html.DisplayFor(model => model.carrera.nom_c)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-primary" /> |
            @Html.ActionLink("Volver", "Index")
        </div>
    }
</div>
