@model IEnumerable<escuelaMVC.Models.materia>

@{
    ViewBag.Title = "Index";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-9" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Nuestas Materias</h1>
            <p class="lead">Aca encuentras toda la lista de las materias, esperamos te gusten.</p>
        </div>

        <div class="col-md-3">
            <img style="height: 300px; width: 250px; margin-right:80px; " src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>

<p style="font-size: 16px;">
    <a class="btn btn-primary" @Html.ActionLink("Agregar Materia", "Create")
    </a>
</p>



<table class="table" style="font-size: 18px;">
    <tr style=" color: #ef6c00;">
        <th>
            Nombre
        </th>
        <th>
            Creditos
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nom_m)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cred_m)
        </td>
        <td style=" color: #ef6c00;">
            @Html.ActionLink("Editar", "Edit", new { id=item.clave_m }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.clave_m })
        </td>
    </tr>
}

</table>
