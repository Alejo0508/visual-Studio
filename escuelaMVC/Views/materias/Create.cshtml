@model escuelaMVC.Models.materia

@{
    ViewBag.Title = "Create";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-7" style="margin-top: 50px; ">
            <h1 style="color: #ef6c00; margin-left: 80px;">Nueva Materia</h1>
            <p class="lead" style="margin-left: 80px;">Estupendo, una nueva materia es mayor conocimiento.</p>
        </div>

        <div class="col-md-5">
            <img style="height: 300px; width: 250px; margin-left: 50px;" src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
<div class="form-horizontal">

    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    <div class="form-group" style="margin-left: 420px;">
        <div class="col-md-10">
            <a style="margin-bottom: 10px; font-size:18px;">Materia</a>
            @Html.EditorFor(model => model.nom_m, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.nom_m, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group" style="margin-left: 220px; margin-top: 40px;" >
        <div class="col-md-5" >
            <a style="margin-bottom: 10px; font-size:18px;">Codigo</a>
            @Html.EditorFor(model => model.clave_m, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.clave_m, "", new { @class = "text-danger" })
        </div>

        <div class="col-md-5">
            <a style="margin-bottom: 10px; font-size:18px;">Creditos</a>
            @Html.EditorFor(model => model.cred_m, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.cred_m, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10" style="margin-left: 530px; margin-top: 40px;">
            <input type="submit" value="Agregar" class="btn btn-primary" />
        </div>
    </div>
</div>
}

<div>
    @Html.ActionLink("Volver", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
