@model escuelaMVC.Models.materia

@{
    ViewBag.Title = "Delete";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-8" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Eliminaremos la materia</h1>
            <p class="lead">No te preocupes, solo es por un tiempo corto.</p>
        </div>

        <div class="col-md-4">
            <img style="height: 300px; width: 250px; margin-right: 50px; " src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>

<div>

    <hr />
    <dl class="dl-horizontal" style="font-size:18px;">
        <dt>
            Nombre
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nom_m)
        </dd>

        <dt>
            Creditos
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cred_m)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-primary" /> |
            @Html.ActionLink("Volver", "Index")
        </div>
    }
</div>
