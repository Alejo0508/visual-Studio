@model escuelaMVC.Models.profesor

@{
    ViewBag.Title = "Create";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-7" style="margin-top: 50px; ">
            <h1 style="color: #ef6c00; margin-left: 80px;">¡Nuevo Profe!</h1>
            <p class="lead" style="margin-left: 80px;">Esperamos aprender mucho.</p>
        </div>

        <div class="col-md-5">
            <img style="height: 300px; width: 250px; margin-left: 50px;" src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <div class="form-group" style="margin-left: 100px;">

            <div class="col-md-4">
                <a style="margin-bottom: 10px; font-size:18px;">Codigo</a>
                @Html.EditorFor(model => model.clave_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.clave_p, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-4">
                <a style="margin-bottom: 10px; font-size:18px;">Nombre</a>
                @Html.EditorFor(model => model.nom_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.nom_p, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-4">
                <a style="margin-bottom: 10px; font-size:18px;">Direccion</a>
                @Html.EditorFor(model => model.dir_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dir_p, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group" style="margin-left: 300px;">
           
            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Telefono</a>
                @Html.EditorFor(model => model.tel_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tel_p, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Horario</a>
                @Html.EditorFor(model => model.hor_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.hor_p, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10" style="margin-left: 600px; margin-top: 40px;">
                <input type="submit" value="Agregar" class="btn btn-primary" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Volver", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
