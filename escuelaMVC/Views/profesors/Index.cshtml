@model IEnumerable<escuelaMVC.Models.profesor>

@{
    ViewBag.Title = "Index";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-8" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Profesores</h1>
            <p class="lead">Mira la informacion del profesor y organiza tus horarios.</p>
        </div>

        <div class="col-md-4">
            <img style="height: 300px; width: 250px; margin-right: 50px; " src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>

<p style="font-size: 16px;">
    <a class="btn btn-primary" @Html.ActionLink("Agregar Profesor", "Create")
    </a>
</p>


<table class="table" style="font-size: 18px;">
    <tr style=" color: #ef6c00;">
        <th>
            Nombre
        </th>
        <th>
            Direccion
        </th>
        <th>
            Telefono
        </th>
        <th>
            Horario
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nom_p)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dir_p)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tel_p)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hor_p)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.clave_p }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.clave_p })
        </td>
    </tr>
}

</table>
