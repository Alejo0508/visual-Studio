@model escuelaMVC.Models.profesor

@{
    ViewBag.Title = "Edit";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-8" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Editar Informacion</h1>
            <p class="lead">Edita la informacion del profesor que necesites.</p>
        </div>

        <div class="col-md-4">
            <img style="height: 300px; width: 250px;" src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.clave_p)

        <div class="form-group" style="margin-left: 200px;">

            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Nombre</a>
                @Html.EditorFor(model => model.nom_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.nom_p, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Direccion</a>
                @Html.EditorFor(model => model.dir_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.dir_p, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group" style="margin-left: 200px;">
            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Telefono</a>
                @Html.EditorFor(model => model.tel_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tel_p, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-5">
                <a style="margin-bottom: 10px; font-size:18px;">Horario</a>
                @Html.EditorFor(model => model.hor_p, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.hor_p, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10" style="margin-left: 520px; margin-top: 40px;">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Volver", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
