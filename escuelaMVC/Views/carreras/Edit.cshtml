@model escuelaMVC.Models.carrera

@{
    ViewBag.Title = "Edit";
}

<div class="jumbotron" style="border:solid 2px #ef6c00">
    <div class="row">
        <div class="col-md-8" style="margin-top: 50px;">
            <h1 style="color: #ef6c00;">Editar Carrera</h1>
            <p class="lead">¿Algo cambio?, esta bien.</p>
        </div>

        <div class="col-md-4">
            <img style="height: 300px; width: 250px;" src="data:image/svg+xml;base64," />
        </div>
    </div>
</div>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">

        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Clave_c)

        <div class="form-group" style="margin-left: 180px;">

            
            <div class="col-md-6">
                <a style="margin-bottom: 10px; font-size:18px;">Nombre</a>
                @Html.EditorFor(model => model.nom_c, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.nom_c, "", new { @class = "text-danger" })
            </div>

            <div class="col-md-6">
                <a style="margin-bottom: 10px; font-size:18px;">Duracion</a>
                @Html.EditorFor(model => model.durac_c, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.durac_c, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group" style="margin-left: 380px; margin-top: 40px;">
            <div class="col-md-offset-2 col-md-10" >
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Volver", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
