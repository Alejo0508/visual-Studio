@model IEnumerable<crudmvc.Models.empleados>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nom_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.edad_empleado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sexo_empledo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.sueldo_empl)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nom_empleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.edad_empleado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sexo_empledo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sueldo_empl)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.cod_empleado }) |
            @Html.ActionLink("Details", "Details", new { id=item.cod_empleado }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.cod_empleado })
        </td>
    </tr>
}

</table>
